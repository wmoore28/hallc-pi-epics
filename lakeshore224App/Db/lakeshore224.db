record(asyn, "$(P)$(R)asyn") {
    field(PORT, "$(PORT)")
    field(OEOS, "\n")
    field(IEOS, "\n")
}

record(ai, "$(P)$(R)TEMP_A") {
    field(DTYP, "stream")
    field(INP,  "@lakeshore224.proto getTemp(A) $(PORT)")
    field(SCAN, ".1 second")
    field(PREC, "2")
    field(EGU,  "C")
}

